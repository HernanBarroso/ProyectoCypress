    "cypress": "^13.2.0",
    "cypress-mochawesome-reporter": "^3.6.0",
    "jest": "^29.5.0",
    "jest-runner-groups": "^2.2.0",
    "node-xlsx": "^0.23.0",
    "typescript": "^5.0.4"
  },
